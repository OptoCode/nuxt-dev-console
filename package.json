{
  "name": "nuxt-dev-console",
  "version": "1.0.0",
  "description": "Development console for Nuxt 3 applications",
  "repository": "your-github-repo",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxt dev playground",
    "build": "nuxt-module-build",
    "lint": "eslint .",
    "test": "vitest run",
    "prepare": "nuxt-module-build",
    "release": "npm run lint && npm run test && npm run build && npm publish",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@nuxt/kit": "^3.x.x"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.x",
    "@nuxt/schema": "^3.x.x",
    "@types/node": "^20.x.x",
    "eslint": "^8.x.x",
    "vitest": "^1.x.x",
    "prettier": "^3.x.x",
    "eslint-config-prettier": "^9.x.x",
    "eslint-plugin-prettier": "^5.x.x"
  }
}
